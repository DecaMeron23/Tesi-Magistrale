/**
 * Libreria per l'utilizzo del led Driver
 */

#ifndef _TLC59731_H
#define _TLC59731_H

#include "main.h"

#define TLC59731_TIM			htim17
#define TLC59731_PWM_PORT
#define TLC59731_PWM_PIN
#define TLC59731_CS_PORT
#define TLC59731_CS_PIN

uint8_t TLC59731_Init();

uint8_t TLC59731_SetRGB(uint8_t red, uint8_t green, uint8_t blue);

uint8_t TLC59731_SendRGB(uint8_t red, uint8_t green, uint8_t blue);

uint8_t TLC59731_On();
uint8_t TLC59731_Off();

uint8_t TLC59731_Toggle();

#endif /* _TLC59731_H */
